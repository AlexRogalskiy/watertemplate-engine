~if mustDisplayMenu:
    <ul id="menu">
        ~for item in items:
            <li>
                <a href="~item.url~"> ~item.label~ </a>
            </li>
        :else:
            <li>
                <span>No items in this menu.</span>
            </li>
        :~
    </ul>
:else:
    <span>Menu unavailable.</span>
:~